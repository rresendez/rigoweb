 mycursor.execute(query)
 global subject
 subject=3Dmycursor.fetchall()
=20

 conn.commit()
 mycursor.close()
 conn.close()
=20
 return subject

def query2(sub):
 import mysql.connector
 =
conn=3Dmysql.connector.connect(user=3D'root',password=3D'Rodriguez24',hos=
t=3D'localhost', database=3D'questions')
 mycursor=3Dconn.cursor()
 query2 =3D ("Select questions, answer  From questions where =
subject=3D'"+ sub +"'")
 mycursor.execute(query2)
 global question
 question=3D mycursor.fetchall()

 conn.commit()
 mycursor.close()
 conn.close()
 from random import randint
 n=3D(randint(0,1))
 print(n)
 question=3D question[n]
 return question


